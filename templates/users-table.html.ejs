<h1>Users</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Email</th>
  </tr>
  <% users.forEach((user) => { %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><a href="/users/<%= user.id %>">Edit</a></td>
    </tr>
  <% }); %>
</table>